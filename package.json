    "i18next-browser-languagedetector": "^7.1.0",
    "object-hash": "^3.0.0",
    "primereact": "^9.6.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "primeicons": "^5.0.0 || ^6.0.0",
    "react-ga4": "^2.1.0",
    "react-i18next": "^13.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "react-social-login-buttons": "^3.9.1",
    "socket.io-client": "4.7.1",
